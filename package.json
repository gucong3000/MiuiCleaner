{
	"name": "miui_cleaner",
	"version": "2022.9.25.5",
	"private": true,
	"description": "MIUI广告清理工具",
	"scripts": {
		"test": "npx eslint *.js src/**/*.js",
		"test:fix": "npm run test -- --fix",
		"start": "npx webpack --config webpack.config.js --watch --mode=development",
		"build:app": "npx webpack --config webpack.config.js --mode=production",
		"build:cmd": "mkdir -p dist/miui_cleaner_cmd && (cat src/miui_cleaner_cmd/main.cmd | iconv --from-code=utf-8 --to-code=gbk > dist/miui_cleaner_cmd/MiuiCleaner.cmd)",
		"build:pull": "adb pull /sdcard/auto.js/miui_cleaner_app/dist/MiuiCleaner_v2022.9.25.5.apk ./dist/miui_cleaner_app/",
		"build": "node project.js && npm run build:app && npm run build:cmd",
		"deploy:json": "cp ./src/miui_cleaner_app/*.json ./dist/miui_cleaner_app/",
		"deploy:app": "adb push ./dist/miui_cleaner_app/ /sdcard/auto.js/",
		"deploy:res": "adb push ./res/ /sdcard/auto.js/miui_cleaner_app/",
		"deploy": "npm run deploy:json && npm run deploy:app && npm run deploy:res",
		"dump:ui": "adb shell uiautomator dump && (adb shell cat /sdcard/window_dump.xml | xmllint --format - > window_dump.xml) && code window_dump.xml",
		"dump:act": "adb shell dumpsys activity activities | grep Hist",
		"dump": "npm run dump:ui && npm run dump:act"
	},
	"author": "GuCong",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.19.0",
		"@babel/plugin-transform-runtime": "^7.18.10",
		"@babel/preset-env": "^7.19.0",
		"@babel/preset-react": "^7.18.6",
		"babel": "^6.23.0",
		"babel-loader": "^8.2.5",
		"buble-config-rhino": "^0.1.0",
		"buble-loader": "^0.5.1",
		"clean-webpack-plugin": "^4.0.0",
		"eslint": "^8.23.1",
		"eslint-config-standard": "^17.0.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-n": "^15.2.5",
		"eslint-plugin-promise": "^6.0.1",
		"eslint-plugin-react": "^7.31.8",
		"file-loader": "^6.2.0",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0"
	}
}
